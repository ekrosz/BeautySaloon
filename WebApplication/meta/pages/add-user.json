                    }
                  ],
                  "events": {
                    "submit": [
                      {
                        "error": [
                          {
                            "detail": "Unable to create new User!",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "LocalDb.createUser",
                        "parameters": [
                          {
                            "name": "User",
                            "value": "${user}"
                          }
                        ],
                        "then": [
                          {
                            "type": "close-popup",
                            "result": "${user}"
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "data": "${user}",
                  "visible": "${user != null}",
                  "itemType": "WebApplication.Models.LocalDb.User"
                }
